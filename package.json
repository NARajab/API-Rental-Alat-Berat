{
  "name": "pmpl",
  "version": "1.0.0",
  "description": "API aplikasi rental alat berat",
  "main": "index.js",
  "scripts": {
    "start": "node -r dotenv/config bin/www",
    "dev": "nodemon -r dotenv/config bin/www",
    "db:drop": "sequelize db:drop",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:migrate:undo": "sequelize db:migrate:undo:all",
    "test": "jest --setupFiles dotenv/config --coverage --colors --verbose --detectOpenHandles --forceExit"
  },
  "author": "Nur Ali Rajab",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "imagekit": "^5.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^22.3.0",
    "qrcode-terminal": "^0.12.0",
    "sequelize": "^6.36.0",
    "sequelize-cli": "^6.6.2",
    "whatsapp-web.js": "^1.23.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  }
}
